{
    "name": "milieu/milieu",
    "description": "An environment initialization, device detection and app configuration library.",
    "keywords": ["environment", "configuration", "detection", "device", "config", "env", "mobile", "tablet", "android", "ipad", "iphone"],
    "author": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas.stendahl@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/finder": "2.2.*"
    },
    "require-dev": {
        "jamesmoss/toml": "dev-master",
        "mockery/mockery": "0.7.*",
        "phpunit/phpunit": "3.7.*",
        "symfony/yaml": "2.2.0-RC3"
    },
    "suggest": {
        "symfony/yaml": "If you want to use config files stored in the YAML format",
        "jamesmoss/toml": "If you want to use config files stored in the TOML format"
    },
    "autoload": {
        "psr-0": {
            "Milieu": "lib/"
        }
    }
}
